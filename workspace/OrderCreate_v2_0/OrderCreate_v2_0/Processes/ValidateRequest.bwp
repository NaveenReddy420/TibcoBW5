;quot;&quot;>&#xa;                        &lt;xsl:if test=&quot;(not(exists(current()/globalskuid)) or tib:trim(current()/globalskuid) = &amp;quot;&amp;quot;) and (not(exists(current()/ingrampartnumber)) or tib:trim(current()/ingrampartnumber) = &amp;quot;&amp;quot;)&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;globalskuid or ingrampartnumber is mandatory at &amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                        &lt;xsl:if test=&quot;not(exists(current()/quantity)) or tib:trim(current()/quantity) = &amp;quot;&amp;quot;&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;quantity is mandatory at Linumber&amp;quot;, &amp;quot;  &amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                    &lt;/xsl:when>&#xa;                    &lt;xsl:when test=&quot;current()/linetype = &amp;quot;N&amp;quot;&quot;>&#xa;                        &lt;xsl:if test=&quot;not(exists(current()/quantity)) or tib:trim(current()/quantity) = &amp;quot;&amp;quot;&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;UnitPrice Entered is not Valid Number at Linumber&amp;quot;, &amp;quot;  &amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                        &lt;xsl:if test=&quot;not(number(current()/unitprice))&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;unit price  is mandatory for non impulse sku at &amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                        &lt;xsl:if test=&quot;not(exists(current()/unitprice)) or tib:trim(current()/unitprice) = &amp;quot;&amp;quot;&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;unit price  is mandatory for non impulse sku at &amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                    &lt;/xsl:when>&#xa;                    &lt;xsl:when test=&quot;current()/linetype = &amp;quot;C&amp;quot;&quot;>&#xa;                        &lt;xsl:if test=&quot;not(exists(current()/productextendedspecs[attributename = &amp;quot;commenttext&amp;quot;]))&quot;>&#xa;                            &lt;mandatoryField>&#xa;                                &lt;xsl:value-of select=&quot;concat(&amp;quot;Comment text is mandatory for C type line&amp;quot;, string(current()/linenumber))&quot;/>&#xa;                            &lt;/mandatoryField>&#xa;                        &lt;/xsl:if>&#xa;                    &lt;/xsl:when>&#xa;                    &lt;xsl:when test=&quot;(not(exists(current()/linetype)) or tib:trim(current()/linetype) = &amp;quot;&amp;quot;) and (not(exists(current()/linetype)) or tib:trim(current()/linetype) = &amp;quot;&amp;quot;)&quot;>&#xa;                        &lt;mandatoryField>&#xa;                            &lt;xsl:value-of select=&quot;&amp;quot;linetype is mandatory to create order for impulse&amp;quot;&quot;/>&#xa;                        &lt;/mandatoryField>&#xa;                    &lt;/xsl:when>&#xa;                &lt;/xsl:choose>&#xa;            &lt;/xsl:for-each>&#xa;        &lt;/ansbw6:mandatoryCheck>&#xa;    &lt;/xsl:template>&#xa;&lt;/xsl:stylesheet>" expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xslt1.0"/>
